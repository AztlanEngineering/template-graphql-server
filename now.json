{
  "regions": ["bru"],
  "routes": [
    { "src": "/.*", "dest": "api/server.js" }
  ],
	"env": {
    "CORS":"false",
		"DEBUG":"true",
		"DB_DEBUG":"true",
		"DB_PORT":"@fwrlines-db-port",
		"DB_HOST":"@fwrlines-db-host",
		"DB_NAME":"@fwrlines-db-name",
		"DB_PASSWORD":"@fwrlines-db-password",
		"DB_USER":"@fwrlines-db-user",
		"JWT_SECRET":"@jwt",
		"SIGNUP":"false",
    "GOOGLE_OAUTH_CLIENT":"@oauth_client",
    "GOOGLE_OAUTH_KEY":"@oauth_secret",
    "GOOGLE_OAUTH_HOST_AUTHORIZE":"https://api.fwrlines.com",
    "GOOGLE_OAUTH_HOST_TOKEN":"https://template-react-dashboard.fwrlines.com",
    "SESSION_DURATION":"86400",
    "TIME_TO_LOGIN":"140"
	}
}
