{
  "verbose": false,
  "ignore": ["node_modules"],
  "env": {
    "NODE_ENV": "development",
    "BABEL_DISABLE_CACHE": 1,
    "LOCAL":true
  },
  "execMap": {
    "js": "babel-node --config-file ./babel.config.js"
  },
  "ext": ".js,.md,.graphql",
  "ignore":["**/test", "**/tests", "**/*.migration.js", "**/scripts"],
  "watch": "./src/"
}
